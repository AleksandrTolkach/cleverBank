<configuration>
  <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d %level [%thread] %logger{10} [%file:%line] %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="com.mchange.v2" level="WARN"/> <!-- Установите уровень логирования в WARN или выше -->

  <root level="INFO">
    <appender-ref ref="CONSOLE"/>
  </root>
</configuration>